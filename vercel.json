{
  "version": 2,
  "builds": [
    {
      "src": "manage.py",
      "use": "@vercel/python"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "marketplace.settings",
    "PYTHONUNBUFFERED": "1"
  },
  "buildCommand": "pip install -r requirements.txt && python manage.py collectstatic --noinput",
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "manage.py"
    }
  ]
}
